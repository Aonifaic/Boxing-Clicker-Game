<!DOCTYPE HTML>
<HTML>
	<head>
		<title>Boxing Clicker Game</title>
		<link rel="stylesheet" href="main.css">
	</head>
	<body>
		<div class="sectionLeft unselectable">
			<div id="menu">
				<div id="resetButton"></div>
			</div>
			<center>
				<div id="scoreContainer">
					KO Points: <span id="score">0</span><br>
					KPs per second: <span id="scorePerSecond">0</span>/s<br>
				</div>
				<div class="unselectable" id="clicker"></div>
			</center>
		</div>
		<div class="sectionRight unselectable">
			<div class="shopButton">
				<div id="shopContainer"></div>				
				<div id="upgradeContainer"></div>
				<div id="achievementContainer></div>
			</div>
		</div>
		
		<script src="main.js"></script><script src="main.js"></script>
	</body>
</HTML>